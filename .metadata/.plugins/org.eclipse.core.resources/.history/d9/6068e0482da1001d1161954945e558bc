<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>joinResult.html</title>
</head>
<body>
<div id="d_result"></div>
	<script type="text/javascript">
	const user_id = 'kiwi';
	const user_pw = '12345';
	const myHeaders = new Headers();
	myHeaders.append("Cookie", "JSESSIONID=0111CAC524921F1909E0A4FC2AFC3FBA");

	const requestOptions = {
	  method: 'GET',
	  headers: myHeaders,
	  redirect: 'follow'
	};

	fetch(`http://localhost:9000/chat221228/member/joinAction.jsp?mem_id=${user_id}&mem_pw=${user_pw}`, requestOptions)
	  .then(response => response.text())
	  .then(result => {
		/* 결과를 여기에 삽입 */
		  const dresult = document.querySelector("#d_result");
		  dresult.innerText = result;
	  })
	  .catch(error => console.log('error', error));
	</script>
</body>
</html>